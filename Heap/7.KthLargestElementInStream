https://leetcode.com/problems/kth-largest-element-in-a-stream/

class KthLargest {
    PriorityQueue<Integer> pq;
    int K;
    public KthLargest(int k, int[] nums) {
        pq = new PriorityQueue<>();
        this.K=k;
        for(int i:nums){
            pq.add(i);
            if(pq.size()>k){
                pq.remove();
            }
        }
    }
    
    public int add(int val) {
        pq.add(val);
        if(pq.size()>K){
            pq.remove();
        }
        return pq.peek();
    }
}
